<resources>

    <!-- Base application theme. -->
    <style label="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item label="colorPrimary">@color/primary</item>
        <item label="colorPrimaryDark">@color/primary</item>
        <item label="colorAccent">@color/primary</item>
        <item label="colorControlNormal">#00FFFFFF</item>
        <item label="colorControlActivated">@color/bluegrey_700</item>
        <item label="colorControlHighlight">#876333</item>
    </style>

    <style label="AppTheme.NoActionBar">
        <item label="windowActionBar">false</item>
        <item label="windowNoTitle">true</item>
    </style>

    <style label="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style label="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

</resources>
